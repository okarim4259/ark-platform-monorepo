{
    "name": "api",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec \"ts-node\" src/index.ts",
        "debug": "nodemon",
        "inspect": "node --inspect=5858 -r ts-node/register ./src/index.ts",
        "build": "rm -rf dist && tsc",
        "start": "node ./dist/index.js",
        "start:prod": "npm run build && npm run start"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "npm run inspect",
        "ext": "ts"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "inversify": "^5.0.1",
        "inversify-express-utils": "^6.3.2",
        "pg": "^7.17.0",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.2.22"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/body-parser": "^1.17.1",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.2",
        "@types/node": "^13.1.1",
        "nodemon": "^2.0.2",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.4"
    }
